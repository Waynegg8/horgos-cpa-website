{% if seo_content and seo_content.enabled %}
<div class="prose prose-sm mx-auto text-center text-gray-500 mt-8">
  <h3>{{ seo_content.title }}</h3>
  <p>{{ seo_content.paragraph }}</p>
  <div class="flex justify-center flex-wrap gap-x-4 gap-y-2">
    {% for link in seo_content.links %}
      <a href="{{ link.url | url }}" title="{{ link.text }}">{{ link.text }}</a>
    {% endfor %}
  </div>
</div>
{% endif %}