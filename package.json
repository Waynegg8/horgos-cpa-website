{
  "name": "horgos-cpa-website",
  "version": "1.0.0",
  "description": "Horgos CPA Website",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build:css-watch & python scripts/process_json.py && npx @11ty/eleventy --serve",
    "build": "python scripts/process_json.py && npx @11ty/eleventy && npm run build:css",
    "build:css": "tailwindcss -i ./assets/css/styles.css -o ./_site/assets/css/styles.css --minify",
    "build:css-watch": "tailwindcss -i ./assets/css/styles.css -o ./_site/assets/css/styles.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.3"
  }
}
